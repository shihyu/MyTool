operators= - ! % & * * / : ; ? ^ | + < = >

;=============================================================================;
[Matlab]
idchars=a-zA-Z_$ 0-9
case-sensitive=y
styles=sqbackslash idparenfunction matlab
mlcomment=%{ %}
linecomment=%
keywords= break case catch classdef continue else elseif end events for
keywords= function global if methods otherwise parfor persistent properties
keywords= return spmd switch try while
keywords= properties methods enumeration events

;=============================================================================;
[Scala]
case-sensitive=Y
idchars=a-zA-Z_$ 0-9
styles=dqbackslash sqbackslash xhex efloat dqbackslashml idparenfunction javadoc doxygen xmldoc tqmultiline
mlcomment=/** */ documentation
mlcomment=/*! */ documentation
mlcomment=/* */
linecomment=//
keywords= abstract case catch class def
keywords= do else extends false final
keywords= finally for forSome if implicit
keywords= import lazy match new null
keywords= object override package private protected
keywords= return sealed super this throw
keywords= trait try true type val
keywords= var while with yield
punctuation={ }
; operators
operators=! % & * - = + : ; < > ? / * | ^

