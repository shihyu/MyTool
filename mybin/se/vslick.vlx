[XHTML]
;
;   The HTML tags and special characters herein are taken from
;       http://www.sandia.gov/sci_compute/elements.html
;       http://www.sandia.gov/sci_compute/symbols.html
;
;   Netscape also has lists of both HTML tags and special characters but they
;   are subsets of the Sandia list with no additional symbols. See
;       http://developer.netscape.com/library/documentation/htmlguid/tags1.htm
;       http://developer.netscape.com/library/documentation/htmlguid/entities.htm
;
;   Note that there are additional keywords from Sandia for identifying icons.
;   These are not incorporated here.
;
;   R.C. Paulsen 3 May 1997
;   Enhanced by Clark Maurer 1/6/98 combined netscape tags and Sandia
;  4/11/2007 - rb - Added W3C HTML 4 entities.
;
idchars=a-zA-Z?_ 0-9:\-
case-sensitive=Y
styles=nonumbers xml
;
; Legacy special chars are not case sensitive
keywords= &amp; &lt; &gt; &quot;

;
; Character entity references in HTML 4
;
; Taken from W3C spec: http://www.w3.org/TR/REC-html40/sgml/entities.html
;

;
; Character entity references for ISO 8859-1 characters
;

cskeywords= &nbsp; &iexcl; &cent; &pound; &curren; &yen; &brvbar; &sect; &uml; &copy; &ordf; &laquo; &not; &shy; &reg; &macr; &deg; &plusmn; &sup2;
cskeywords= &sup3; &acute; &micro; &para; &middot; &cedil; &sup1; &ordm; &raquo; &frac14; &frac12; &frac34; &iquest; &Agrave; &Aacute; &Acirc; &Atilde;
cskeywords= &Auml; &Aring; &AElig; &Ccedil; &Egrave; &Eacute; &Ecirc; &Euml; &Igrave; &Iacute; &Icirc; &Iuml; &ETH; &Ntilde; &Ograve; &Oacute; &Ocirc;
cskeywords= &Otilde; &Ouml; &times; &Oslash; &Ugrave; &Uacute; &Ucirc; &Uuml; &Yacute; &THORN; &szlig; &agrave; &aacute; &acirc; &atilde; &auml;
cskeywords= &aring; &aelig; &ccedil; &egrave; &eacute; &ecirc; &euml; &igrave; &iacute; &icirc; &iuml; &eth; &ntilde; &ograve; &oacute; &ocirc; &otilde; &ouml;
cskeywords= &divide; &oslash; &ugrave; &uacute; &ucirc; &uuml; &yacute; &thorn; &yuml;

;
; Character entity references for symbols, mathematical symbols, and Greek letters
;

cskeywords= &fnof; &Alpha; &Beta; &Gamma; &Delta; &Epsilon; &Zeta; &Eta; &Theta; &Iota; &Kappa; &Lambda; &Mu; &Nu; &Xi; &Omicron; &Pi; &Rho;
cskeywords= &Sigma; &Tau; &Upsilon; &Phi; &Chi; &Psi; &Omega; &alpha; &beta; &gamma; &delta; &epsilon; &zeta; &eta; &theta; &iota; &kappa;
cskeywords= &lambda; &mu; &nu; &xi; &omicron; &pi; &rho; &sigmaf; &sigma; &tau; &upsilon; &phi; &chi; &psi; &omega; &thetasym; &upsih; &piv;

; General Punctuation

cskeywords= &bull; &hellip; &prime; &Prime; &oline; &frasl;

; Letterlike Symbols

cskeywords= &weierp; &image; &real; &trade; &alefsym;

; Arrows

cskeywords= &larr; &uarr; &rarr; &darr; &harr; &crarr; &lArr; &uArr; &rArr; &dArr; &hArr;

; Mathematical Operators

cskeywords= &forall; &part; &exist; &empty; &nabla; &isin; &notin; &ni; &prod; &sum; &minus; &lowast; &radic; &prop; &infin; &ang; &and; &or; &cap;
cskeywords= &cup; &int; &there4; &sim; &cong; &asymp; &ne; &equiv; &le; &ge; &sub; &sup; &nsub; &sube; &supe; &oplus; &otimes; &perp; &sdot;

; Miscellaneous Technical

cskeywords= &lceil; &rceil; &lfloor; &rfloor; &lang; &rang;

; Geometric Shapes

cskeywords= &loz;

; Miscellaneous Symbols
cskeywords= &spades; &clubs; &hearts; &diams;

;
; Character entity references for markup-significant and internationalization characters
;

; C0 Controls and Basic Latin
; (already defined above as legacy entities)

;cskeywords= &quot; &amp; &lt; &gt;

; Latin Extended-A

cskeywords= &OElig; &oelig; &Scaron; &scaron; &Yuml;

; Spacing Modifier Letters

cskeywords= &circ; &tilde;

; General Punctuation

cskeywords= &ensp; &emsp; &thinsp; &zwnj; &zwj; &lrm; &rlm; &ndash; &mdash; &lsquo; &rsquo; &sbquo; &ldquo; &rdquo; &bdquo; &dagger; &Dagger;
cskeywords= &permil; &lsaquo; &rsaquo; &euro;

