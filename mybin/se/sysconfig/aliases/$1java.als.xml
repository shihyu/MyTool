<profile name="/language/java/aliases">
	<p n="=surround_with_while">
		<text>
			while (%\c) {
			%\m sur_text -indent%
			}
		</text>
	</p>
	<p n="=surround_with_try">
		<text>
			try {
			%\m sur_text -indent%
			}
		</text>
	</p>
	<p n="=surround_with_catch">
		<text>
			catch (%\c) {
			%\m sur_text -indent%
			}
		</text>
	</p>
	<p n="=surround_with_do...while">
		<text>
			do {
			%\m sur_text -indent%
			} while (%\c);
		</text>
	</p>
	<p n="=surround_with_if">
		<text>
			if (%\c) {
			%\m sur_text -indent%
			}
		</text>
	</p>
	<p n="=surround_with_default">
		<text>
			default:
			%\m sur_text -indent%
		</text>
	</p>
	<p n="=surround_with_case">
		<text>
			case %\c:
			%\m sur_text -indent%
			break;
		</text>
	</p>
	<p n="=surround_with_switch">
		<text>
			switch (%\c) {
			%\m sur_text%
			}
		</text>
	</p>
	<p n="=surround_with_if...else">
		<text>
			if (%\c) {
			%\m sur_text -indent%
			} else {
			%\m sur_text -indent%
			}
		</text>
	</p>
	<p n="=surround_with_finally">
		<text>
			finally {
			%\m sur_text -indent%
			}
		</text>
	</p>
	<p n="=surround_with_for">
		<text>
			for (%\c) {
			%\m sur_text -indent%
			}
		</text>
	</p>
</profile>
