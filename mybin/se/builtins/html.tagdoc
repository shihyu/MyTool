 * &lt;jsp:include page="copyright.html" />
 * <P>
 * &lt;jsp:include page="/index.html" />
 */
tag "jsp:include" {
   /**
    * The relative URL to the file to be included, or an expression that
    * evaluates to a String equivalent to the relative URL.
    * <P>
    * The relative URL looks like a pathnameit cannot contain a protocol name,
    * port number, or domain name.  The URL can be absolute or relative to the
    * current JSP file.  If it is absolute (beginning with a /), the pathname is
    * resolved by your Web or application server.
    */
   attr page;
   /**
    * Optional boolean attribute. If the value is true, the buffer is
    * flushed now. The default value is false.
    * In JSP 1.0, you must include flush="true", as it is not a default value.
    */
   attr flush {
      false
