   attr type {
      "code"
      "message"
      "print"
      "text"
      "text/plain"
      "text/html"
      "text/sgml"
      "text/css"
      "text/xml"
      "text/rtf"
      "text/javascript"
      "text/perlscript"
      "text/vbscript"
      "text/php"
      "text/cfscript"
      "text/csharp"
      "text/speedscript"
   }
