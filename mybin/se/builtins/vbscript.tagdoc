 *                         <DT>vbSunday<DD style="marginleft:75pt">(1) -- Sunday (default)
 *                         <DT>vbMonday<DD style="marginleft:75pt">(2) -- Monday
 *                         <DT>vbTuesday<DD style="marginleft:75pt">(3) -- Tuesday
 *                         <DT>vbWednesday<DD style="marginleft:75pt">(4) -- Wednesday
 *                         <DT>vbThursday<DD style="marginleft:75pt">(5) -- Thursday
 *                         <DT>vbFriday<DD style="marginleft:75pt">(6) -- Friday
 *                         <DT>vbSaturday<DD style="marginleft:75pt">(7) -- Saturday
